    <children>
        <Label text="Справочная информация" layoutY="10.0" layoutX="100.0" styleClass="h1"/>
        <ToggleButton styleClass="butt" fx:id="aboutProgramButt" onAction="#handleAboutProgramButton" layoutX="30.0" layoutY="40.0" prefHeight="20.0" prefWidth="100.0" text="О программе"/>
        <ToggleButton styleClass="butt" fx:id="aboutAuthorsButt" onAction="#handleAboutAuthorsButton" layoutX="140.0" layoutY="40.0" prefHeight="20.0" prefWidth="100.0" text="Об авторах"/>
        <Label styleClass="helpLabel" layoutY="70.0" layoutX="30.0" prefWidth="340.0" prefHeight="400.0" fx:id="helpLabel"/>
